@model WebGameMVC.Models.EF.Dungeon


<div class="row">
    <div class="col-md-4">
        <span>ID: </span>
    </div>
    <div class="col-md-8">
        @Model.ID
    </div>
</div>
<div class="row">
    <img style="width:100%;height:100%" src="@Model.Background" />
</div>
<div class="row">
    <div class="col-md-4">
        <span>Tên chương: </span>
    </div>
    <div class="col-md-8">
        @Model.NameDungeon
    </div>
</div>
<div class="row">
    <div class="col-md-4">
        <span>Trạng thái: </span>
    </div>
    <div class="col-md-8">
        @Model.Status
    </div>
</div>
<div class="row">
    <div class="col-md-12">
        <a href="/Admin/Question/ListStage?dungeonID=@Model.ID" class="btn btn-success">Xem ải chương @Model.ID</a>
    </div>
</div>